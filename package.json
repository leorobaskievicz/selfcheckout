{
  "productName": "selfcheckout",
  "artifactName": "${productName}.${ext}",
  "version": "1.1.2",
  "description": "Caixa Self Checkout CallFarma",
  "iniName": "ADMINH.ini",
  "homepage": "https://callfarma.com.br",
  "main": "./src/main/main.js",
  "scripts": {
    "build": "node pre-build.js && concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts && cp src/renderer/loading.html release/app/dist/renderer/ && cp assets/loader-caixa.gif release/app/dist/renderer/ && cp assets/logo-callfarma.png release/app/dist/renderer/ && mkdir release/app/dist/renderer/assets && cp ./assets/audio/*.mp3 release/app/dist/renderer/assets/ && cp -r ./assets/cupom release/app/dist/renderer/assets ",
    "postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx",
    "package": "NODE_ENV=production ts-node ./.erb/scripts/clean.js dist && NODE_ENV=production npm run build && NODE_ENV=production electron-builder build --publish never --win",
    "packagePublish": "NODE_ENV=production ts-node ./.erb/scripts/clean.js dist && NODE_ENV=production npm run build && NODE_ENV=production electron-builder build --publish always --win",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only .",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "start-react": "webpack-dev-server --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "jest"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "dependencies": {
    "@electron/remote": "^2.0.10",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "react-qr-code": "^2.0.15"
  },
  "devDependencies": {
    "@electron/notarize": "^1.2.3",
    "@electron/rebuild": "^3.2.13",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/fira-sans": "^5.0.5",
    "@fontsource/roboto": "^5.0.5",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "^8.0.1",
    "@sweetalert/with-react": "^0.1.1",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/react-redux": "^7.1.25",
    "@types/react-test-renderer": "^18.0.0",
    "@types/terser-webpack-plugin": "^5.0.4",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "axios": "^1.4.0",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^4.1.2",
    "concurrently": "^8.1.0",
    "core-js": "^3.30.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "detect-port": "^1.5.1",
    "electron": "^32.1.2",
    "electron-builder": "^24.13.3",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^17.1.2",
    "electron-pos-printer": "1.3.6",
    "electron-prebuilt": "^1.4.13",
    "electronmon": "^2.0.2",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-erb": "^4.0.6",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "identity-obj-proxy": "^3.0.0",
    "ini": "^4.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.6",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "prettier": "^2.8.8",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "react-webcam": "^7.2.0",
    "redux": "^4.2.1",
    "redux-devtools": "^3.7.0",
    "redux-persist": "^6.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.1",
    "style-loader": "^3.3.3",
    "sweetalert": "^2.1.2",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typesafe-actions": "^5.1.0",
    "typescript": "^5.1.3",
    "url-loader": "^4.1.1",
    "uuid": "^9.0.0",
    "webcamjs": "^1.0.26",
    "webpack": "^5.85.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0",
    "webpack-merge": "^5.9.0"
  },
  "build": {
    "buildVersion": "1.1.2",
    "npmRebuild": false,
    "productName": "selfcheckout",
    "artifactName": "${productName}.${ext}",
    "appId": "br.com.callfarma.caixa",
    "publish": [
      {
        "provider": "github",
        "owner": "leorobaskievicz",
        "repo": "selfcheckout",
        "releaseType": "release"
      }
    ],
    "compression": "maximum",
    "asar": true,
    "asarUnpack": "**\\*.{dll}",
    "files": [
      "dist",
      "assets",
      "package.json",
      "app.html",
      "main.prod.js",
      "main.prod.js.map"
    ],
    "afterSign": ".erb/scripts/notarize.js",
    "win": {
      "artifactName": "${productName}.${ext}",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**"
    ],
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "allowElevation": true,
      "runAfterFinish": true,
      "deleteAppDataOnUninstall": true
    }
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/**"
    ],
    "logLevel": "quiet"
  }
}
